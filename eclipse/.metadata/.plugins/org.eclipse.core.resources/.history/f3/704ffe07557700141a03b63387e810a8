package com.juanwan11_magicnation.recipies.botania;

import com.juanwan11_magicnation.Items.MAItems;
import com.juanwan11_magicnation.recipies.CraftingRecipies;

import vazkii.botania.api.BotaniaAPI;
import net.minecraft.item.ItemStack;

public class CraftingRecipes extends CraftingRecipies{
	public static RecipeManaInfusion bloodRecipe;
	
	
	public static void BotaniaCraftingRecipe() {
		bloodRecipe = BotaniaAPI.registerManaAlchemyRecipe(new ItemStack(MAItems.itemGemStoneBlood), shardGemBloodStack, 16000);
	}
}
